package bgu.dl.features.learning;

import java.util.ArrayList;

import pddl4j.PDDLObject;
import pddl4j.exp.AtomicFormula;

/**
 * @author Shashank Shekhar
 * BGU of the Negev
 */
public class PossibleLiterals {
	PDDLObject pddlObject = null;
	public PossibleLiterals(PDDLObject pddlObject){
		this.pddlObject = pddlObject;
	}
	ArrayList<AtomicFormula> listOfUngroundedLiterals = pddlObject.
}
