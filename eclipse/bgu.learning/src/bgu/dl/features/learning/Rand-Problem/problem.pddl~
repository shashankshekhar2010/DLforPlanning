(define (problem BLOCKS-4-0)
(:domain BLOCKS)
(:objects  A B C D E F G H I J)
(:init (HANDEMPTY)(ON A I)(ON B J)(ONTABLE C)(ON D B)(CLEAR D)(ON E F)(CLEAR E)(ON F A)(ON G C)(ON H G)(ONTABLE I)(ON J H))
(:goal (AND (HANDEMPTY)(ONTABLE A)(ON B D)(ON C F)(ON D J)(ON E A)(ON F B)(ON G E)(ON H G)(CLEAR H)(ON I C)(CLEAR I)(ONTABLE J)))
)
